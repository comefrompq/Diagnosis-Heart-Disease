@model DHD_System.Models.DiagnosisViewModels.TestViewModel
@if (Model.ListOptions != null)
{
    <div class="list-group">
        @foreach (var item in Model.ListOptions)
        {
            <a asp-action="TreatMentFor" asp-controller="Diagnosis" asp-route-Disease=@item class="list-group-item list-group-item-action"
                  data-ajax="true"
                  data-ajax-method="GET"
                  data-ajax-mode="replace"
                  data-ajax-update="#treatment">@item</a>
        }
    </div>
}

